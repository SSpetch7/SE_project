<Button>:
    background_normal: ''
    background_color: (244/255, 132/255, 0, 1)
    font_name: "Montserrat/Montserrat-Bold.ttf"

<MenuButton@Button>:
    background_color: (0, 0, 0, 0)
    background_normal: ''
    font_size: 40 * (root.height * 0.01)
    size_hint_x: 0.9
    size_hint_min_y: 35
    bold: True
    canvas.before:
        Color:
            rgba: (244/255, 132/255, 0, 1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [6,0,0,6]

<CurrencyLabel@Label>:
    halign: "right"
    valign: 'middle'
    pos_hint: {"top": 1}
    color: (1, 1, 1, 1)
    bold: True
    font_size: 24
    text_size: self.size
    size_hint: None, 0.45
    width: 200
    canvas.before:
        Color:
            rgba: (244/255, 132/255, 0, 1)
        Rectangle:
            size: self.size
            pos: self.pos

